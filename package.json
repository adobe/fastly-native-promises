{
  "name": "@adobe/fastly-native-promises",
  "version": "2.0.8",
  "description": "Promise based Fastly API client for Node.js",
  "main": "src/index.js",
  "scripts": {
    "commit": "npm run lint && npm run docs && git-cz",
    "test": "nyc mocha -p",
    "lint": "./node_modules/.bin/eslint .",
    "junit": "nyc mocha --no-color --exit -R xunit --reporter-option output=junit/test-results.xml",
    "docs": "jsdoc2md --files src/*.js -t README.md.hbs -d 3 > README.md",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/fastly-native-promises.git"
  },
  "keywords": [
    "fastly",
    "promises",
    "api",
    "client",
    "cdn"
  ],
  "author": "Philipp Schulte <philipp.schulte@ymail.com> (http://www.philippschulte.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adobe/fastly-native-promises/issues"
  },
  "homepage": "https://github.com/adobe/fastly-native-promises#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.3.2",
    "@adobe/helix-testutils": "0.4.13",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.17.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.2",
    "expect": "28.1.0",
    "ghooks": "2.0.4",
    "jsdoc-to-markdown": "7.1.1",
    "junit-report-builder": "3.0.0",
    "lodash": "4.17.21",
    "mocha": "10.0.0",
    "nock": "13.2.6",
    "nyc": "15.1.0",
    "semantic-release": "19.0.2"
  },
  "dependencies": {
    "@adobe/helix-fetch": "3.0.10",
    "form-data": "4.0.0",
    "object-hash": "3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
